{
  "name": "lunar-events",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "devDependencies": {
    "eslint": "^3.16.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "react-scripts": "0.9.0",
    "react-test-renderer": "^15.4.2",
    "redux-mock-store": "^1.2.2"
  },
  "dependencies": {
    "@panosoft/ramda-utils": "^0.2.8",
    "apollo-client": "^0.9.0",
    "graphql-tag": "^1.2.4",
    "immutable": "^3.8.1",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-apollo": "^0.12.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-logger": "^2.8.1",
    "reselect": "^2.5.4",
    "sw-precache": "^5.0.0"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "react/jsx-indent": [
        "error",
        4
      ],
      "react/jsx-indent-props": [
        "error",
        4
      ]
    }
  },
  "scripts": {
    "start": "NODE_PATH=src react-scripts start",
    "build": "NODE_PATH=src react-scripts build && sw-precache --config=sw-precache-config.js",
    "build:clean": "rimraf build",
    "test": "NODE_PATH=src react-scripts test --env=jsdom",
    "lint:js": "eslint src",
    "lint:js:fix": "eslint src --fix",
    "eject": "react-scripts eject",
    "deploy": "npm run build:clean && npm run build && aws --profile jdinhlifeapps s3 sync --delete build/ s3://apps-jdinh-life/apps/lunar-events/"
  }
}
